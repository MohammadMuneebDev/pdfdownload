{
    "version": 2,
    "functions": {
      "api/*.js": {
        "memory": 1024,
        "maxDuration": 60
      }
    },
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/download",
        "dest": "/api/download.js",
        "methods": ["POST"]
      },
      {
        "src": "/",
        "dest": "/api/index.js"
      }
    ]
  }
  